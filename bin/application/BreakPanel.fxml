<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.BreakPanelController">
   <children>
      <AnchorPane layoutY="-12.0" prefHeight="300.0" prefWidth="400.0" style="-fx-background-color: #DD9C7C#DD9C7C;">
         <children>
            <Button fx:id="notYetButton" layoutX="61.0" layoutY="145.0" mnemonicParsing="false" onAction="#not_yet" text="KEEP GOING ON.  But take a break first." textFill="#4f3838">
               <font>
                  <Font name="Avenir Book" size="15.0" />
               </font>
            </Button>
            <Button fx:id="stopFinishButton" layoutX="112.0" layoutY="211.0" mnemonicParsing="false" onAction="#stop_finish_button" text="I have finished my task." textFill="#4f3838">
               <font>
                  <Font name="Avenir Book" size="15.0" />
               </font>
            </Button>
            <Text fill="WHITE" layoutX="24.0" layoutY="60.0" strokeType="OUTSIDE" strokeWidth="0.0" text="You have gone through ">
               <font>
                  <Font name="Avenir Book" size="20.0" />
               </font>
            </Text>
            <Text fill="WHITE" layoutX="24.0" layoutY="92.0" strokeType="OUTSIDE" strokeWidth="0.0" text="the work time...">
               <font>
                  <Font name="Avenir Book" size="24.0" />
               </font>
            </Text>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
